#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/m@1.9.0/node_modules/m/bin/node_modules:/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/m@1.9.0/node_modules/m/node_modules:/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/m@1.9.0/node_modules:/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/m@1.9.0/node_modules/m/bin/node_modules:/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/m@1.9.0/node_modules/m/node_modules:/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/m@1.9.0/node_modules:/mnt/c/Users/pt/Desktop/item/create-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../../bin/m" "$@"
else
  exec bash  "$basedir/../../bin/m" "$@"
fi
